{
  "name": "@medyll/slotui",
  "version": "0.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/medyll/slot-ui.git"
  },
  "scripts": {
    "dev": "svelte-kit dev  --port 3001 --host",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "powerShell": "Start-process powershell -Verb runAs",
    "css-build": "sass sass/cssfabric.scss cssfabric/cssfabric.vars.css",
    "css-watch": "npm run css-build -- --watch",
    "precommit": "npm unlink @medyll/cssfabric && npm i @medyll/cssfabric ",
    "postcommit": "npm remove @medyll/cssfabric & npm link --save @medyll/cssfabric",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@medyll/cssfabric": "^0.1.12",
    "@neodrag/svelte": "^1.1.2",
    "@sveltejs/kit": "^1.0.0-next.311",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltestack/svelte-query": "^1.6.0",
    "axios": "^0.25.0",
    "lodash": "^4.17.21",
    "svelte-css-vars": "^0.0.3",
    "svelte-fa": "^2.4.0",
    "svelte-spa-router": "^3.2.0",
    "svelte-themer": "^0.5.5",
    "svelte-use-form": "^2.4.3",
    "svelte2tsx": "^0.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.67",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.36",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/svelte": "^3.1.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/color": "^3.0.3",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@vitebook/client": "^0.22.0",
    "@vitebook/core": "^0.22.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "jest": "^27.5.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.7.0"
  }
}
