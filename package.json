{
	"name": "@medyll/slot-ui",
	"version": "0.1.1-beta.0",
	"scope": "@medyll",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"build-docs": "node build-docs.js",
		"prepare": "husky install",
		"release": "standard-version",
		"release:alpha": "standard-version --prerelease alpha",
		"release:beta": "standard-version --prerelease beta",
		"release:minor": "standard-version --release-as minor",
		"publish": "npm run package",
		"slotui-book": "nodemon --watch ./src/lib --watch ./extract-types.js --ignore **/docs/** --ignore ./src/lib/svelte.index.js --ext svelte --delay 2.5 extract-types.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "2.2.1",
		"svelte": "4.2.8",
		"svelte-use-form": "2.10.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.4",
		"@commitlint/config-conventional": "^18.4.4",
		"@medyll/cssfabric": "^0.4.1",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/kit": "^2.2.1",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/fs-extra": "^11.0.4",
		"@types/prismjs": "^1.26.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"commitizen": "^4.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"fs-extra": "^11.2.0",
		"husky": "^8.0.0",
		"mdsvex": "^0.11.0",
		"micromatch": "^4.0.5",
		"nodemon": "^3.0.3",
		"pm2": "^5.3.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prism": "^4.1.2",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"publint": "^0.1.9",
		"query-string": "^8.1.0",
		"rimraf": "^5.0.5",
		"sass": "^1.69.7",
		"standard-version": "^9.5.0",
		"sveld": "^0.19.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-use-form": "^2.10.0",
		"svelte2tsx": "^0.7.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"vitest": "^1.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"d3": "^7.8.5",
		"iconify-icon": "^1.0.8",
		"postcss-load-config": "^5.0.2",
		"sanitize-html": "^2.11.0",
		"svelte-spa-router": "^4.0.1"
	}
}
