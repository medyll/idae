{
  "name": "@medyll/idae-htmlu",
  "version": "0.1.3",
  "author": "Lebrun Meddy",
  "description": "htmlu : vite preprocess utility allowing to use classNames as elements tags. The next step of utility classNames",
  "main": "./dist/index.js",
  "bin": {
    "idae-htmlu": "./dist/bin/bin.js"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "prepackage": "node scripts/package-pre.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-auto": "^7.0.1",
    "@sveltejs/kit": "^2.51.0",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/jsdom": "^27.0.0",
    "publint": "^0.3.17",
    "svelte": "^5.51.0",
    "svelte-check": "^4.4.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scope": "medyll",
  "dependencies": {
    "command": "^1.1.1",
    "commander": "^14.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medyll/idae.git"
  },
  "publishConfig": {
    "access": "public",
    "directory": "."
  }
}
