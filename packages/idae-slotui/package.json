{
  "name": "@medyll/idae-slotui-svelte",
  "bin": {
    "idae-slotui-svelte": "./scripts/cli.js"
  },
  "author": "Lebrun Meddy",
  "version": "0.181.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medyll/idae.git"
  },
  "scope": "medyll",
  "description": "A Svelte 5 component library for building modular and reactive user interfaces.",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "test:unit:docs": "vitest src/tests/docs.test.js",
    "publish": "npm run package",
    "build-docs:watch": "nodemon --watch ./src/lib --watch ./build-docs.js --ignore **/docs/** --ignore ./src/lib/svelte.index.js --ext svelte --delay 2.5 build-docs.js",
    "rename-scss": "node scripts/rename-scss-files.js",
    "package:pre": "node scripts/package-pre.js",
    "prepackage": "node scripts/package-pre.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "."
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.53.0",
    "svelte": "^5.0.0-next"
  },
  "devDependencies": {
    "@medyll/cssfabric": "0.2.1-beta.9",
    "@medyll/idae-be": "workspace:*",
    "@medyll/idae-dom-events": "workspace:*",
    "@medyll/idae-engine": "workspace:*",
    "@medyll/idae-eslint-config": "workspace:*",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-auto": "^7.0.1",
    "@sveltejs/kit": "^2.53.0",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^7.0.0",
    "@types/eslint": "9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/prismjs": "^1.26.6",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "commitizen": "^4.3.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.15.0",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.6",
    "jsdoc": "^4.0.5",
    "mdsvex": "^0.12.6",
    "micromatch": "^4.0.8",
    "nodemon": "^3.1.14",
    "pm2": "^6.0.14",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.5.0",
    "prism": "^4.1.2",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.30.0",
    "query-string": "^9.3.1",
    "rimraf": "^6.1.3",
    "sveld": "^0.26.2",
    "svelte-check": "^4.4.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "idae",
    "slotui",
    "svelte",
    "svelte5",
    "ui",
    "component-library",
    "unocss"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@iconify/svelte": "^5.2.1",
    "@tailwindcss/postcss": "^4.2.1",
    "@tailwindcss/vite": "^4.2.1",
    "autoprefixer": "^10.4.24",
    "commander": "^14.0.3",
    "d3": "^7.9.0",
    "lerna": "^9.0.4",
    "minimatch": "^10.2.2",
    "npm-check-updates": "^19.4.1",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "sanitize-html": "^2.17.1",
    "svelte-preprocess": "^6.0.3",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1"
  }
}
