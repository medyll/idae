{
	"name": "@medyll/idae-slotui-svelte",
	"version": "0.81.0",
	"scope": "@medyll",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"publish": "npm run package",
		"build-docs:watch": "nodemon --watch ./src/lib --watch ./build-docs.js --ignore **/docs/** --ignore ./src/lib/svelte.index.js --ext svelte --ext scss --delay 2.5 build-docs.js",
		"rename-scss": "node scripts/rename-scss-files.js",
		"package:pre": "node scripts/package-pre.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next"
	},
	"devDependencies": {
		"@medyll/idae-prettier-config": "^1.1.0",
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "9.6.1",
		"@types/fs-extra": "^11.0.4",
		"@types/prismjs": "^1.26.5",
		"@types/sanitize-html": "^2.13.0",
		"@typescript-eslint/eslint-plugin": "^8.28.0",
		"@typescript-eslint/parser": "^8.28.0",
		"@unocss/preset-mini": "^66.0.0",
		"@unocss/preset-uno": "^66.0.0",
		"@unocss/preset-wind": "^66.0.0",
		"@unocss/svelte-scoped": "^66.0.0",
		"commitizen": "^4.3.1",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"fs-extra": "^11.3.0",
		"glob": "^11.0.1",
		"jsdoc": "^4.0.4",
		"mdsvex": "^0.12.3",
		"micromatch": "^4.0.8",
		"nodemon": "^3.1.9",
		"pm2": "^6.0.5",
		"prettier": "^4.0.0-alpha.8",
		"prettier-plugin-svelte": "^3.3.3",
		"prism": "^4.1.2",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.30.0",
		"query-string": "^9.1.1",
		"rimraf": "^6.0.1",
		"sass": "^1.86.0",
		"sveld": "^0.22.1",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"svelte2tsx": "^0.7.35",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vitest": "^3.0.9"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^4.2.0",
		"@medyll/cssfabric": "^0.4.2",
		"@medyll/idae-be": "^0.80.0",
		"@medyll/idae-engine": "^1.78.0",
		"d3": "^7.9.0",
		"lerna": "^8.2.1",
		"npm-check-updates": "^17.1.16",
		"postcss-load-config": "^6.0.1",
		"sanitize-html": "^2.15.0",
		"svelte-preprocess": "^6.0.3",
		"svelte-spa-router": "^4.0.1"
	}
}