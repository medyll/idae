{
	"name": "@medyll/idae-slotui-svelte",
	"version": "0.44.0",
	"scope": "@medyll",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"publish": "npm run package",
		"build-docs:watch": "nodemon --watch ./src/lib --watch ./build-docs.js --ignore **/docs/** --ignore ./src/lib/svelte.index.js --ext svelte --ext scss --delay 2.5 build-docs.js",
		"rename-scss": "node scripts/rename-scss-files.js",
		"package:pre": "node scripts/package-pre.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next"
	},
	"devDependencies": {
		"@medyll/idae-prettier-config": "^1.1.0",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "9.6.0",
		"@types/fs-extra": "^11.0.4",
		"@types/prismjs": "^1.26.4",
		"@types/sanitize-html": "^2.11.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@unocss/preset-mini": "^0.61.9",
		"@unocss/preset-uno": "^0.61.9",
		"@unocss/preset-wind": "^0.61.9",
		"@unocss/svelte-scoped": "^0.61.9",
		"commitizen": "^4.3.0",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"fs-extra": "^11.2.0",
		"glob": "^11.0.0",
		"jsdoc": "^4.0.3",
		"mdsvex": "^0.11.2",
		"micromatch": "^4.0.7",
		"nodemon": "^3.1.4",
		"pm2": "^5.4.2",
		"prettier": "^4.0.0-alpha.8",
		"prettier-plugin-svelte": "^3.2.6",
		"prism": "^1.0.0",
		"prism-svelte": "^0.5.0",
		"prismjs": "^1.29.0",
		"query-string": "^9.1.0",
		"rimraf": "^6.0.1",
		"sass": "^1.77.8",
		"sveld": "^0.20.0",
		"svelte": "^5.0.0-next.257",
		"svelte-check": "^3.8.5",
		"svelte2tsx": "^0.7.15",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vitest": "^2.0.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@iconify/svelte": "^4.0.2",
		"@medyll/cssfabric": "^0.4.2",
		"@medyll/idae-be": "^0.43.0",
		"@medyll/idae-engine": "^1.41.0",
		"d3": "^7.9.0",
		"lerna": "^8.1.8",
		"npm-check-updates": "^17.0.2",
		"postcss-load-config": "^6.0.1",
		"sanitize-html": "^2.13.0",
		"svelte-preprocess": "^6.0.2",
		"svelte-spa-router": "^4.0.1"
	}
}
