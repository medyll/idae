{
  "name": "@medyll/idae-cadenzia",
  "version": "0.147.2",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "prepackage": "node scripts/package-pre.js"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "types": "dist/types/index.d.ts",
  "prepare": "npm run prepackage && npm run build:lib",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "."
  },
  "keywords": [
    "idae",
    "cadenzia",
    "ui",
    "component",
    "svelte",
    "plugin"
  ],
  "devDependencies": {
    "@medyll/idae-be": "workspace:*",
    "@medyll/idae-config-prettier": "latest",
    "@medyll/idae-eslint-config": "workspace:*",
    "@medyll/idae-shared": "workspace:*",
    "@semantic-release/github": "^12.0.5",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/eslint": "^9.6.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.50.0",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "scope": "medyll",
  "author": "Lebrun Meddy",
  "dependencies": {
    "command": "^1.1.1",
    "commander": "^14.0.3"
  }
}
