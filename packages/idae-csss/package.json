{
  "name": "@medyll/idae-csss",
  "version": "0.0.1",
  "description": "CSS and Style utility library for the Idae ecosystem.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "prepackage": "node scripts/package-pre.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "keywords": [
    "idae",
    "css",
    "utility",
    "typescript",
    "svelte"
  ],
  "author": "Lebrun Meddy",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "scope": "@medyll",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@medyll/idae-config-prettier": "latest",
    "@medyll/idae-eslint-config": "workspace:*",
    "@medyll/idae-shared": "workspace:*",
    "@sveltejs/adapter-auto": "^5.0.0",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "postcss": "^8.5.6",
    "publint": "^0.3.7",
    "svelte": "^5.25.3",
    "svelte-check": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  }
}
