{
  "name": "@medyll/idae-api",
  "scope": "@medyll",
  "version": "0.158.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "description": "A flexible and extensible API framework for Node.js, designed to work with multiple database types and configurations. It features a modular architecture, dynamic database connection management, and a flexible routing system. The framework supports TypeScript for improved robustness and maintainability.",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "package:pre": "node scripts/package-pre.js",
    "doc": "typedoc --entryPointStrategy expand src/lib/server/middleware src/lib/server/engine src/lib/client"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "tshy": {
    "dialects": [
      "esm"
    ]
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "mongoose-sequence": "^6.0.1",
    "qs": "^6.14.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@medyll/idae-db": "*",
    "@medyll/idae-prettier-config": "*",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.2",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-sequence": "^3.0.11",
    "@types/sinon": "^21.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "mysql2": "^3.14.1",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "sinon": "^21.0.1",
    "supertest": "^7.1.1",
    "svelte": "^5.33.14",
    "svelte-check": "^4.2.1",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.16",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.1",
    "zod": "^3.22.4"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
