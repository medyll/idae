{
  "name": "@medyll/idae-engine",
  "scope": "medyll",
  "author": "Lebrun Meddy",
  "repository": {
    "type": "git",
    "url": "https://github.com/medyll/idae.git"
  },
  "version": "1.185.2",
  "description": "A powerful TypeScript library for data manipulation and operations across the idae-engine. Provides utility classes for sorting, finding, grouping, and more.",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "prepackage": "node scripts/package-pre.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0-next"
  },
  "publishConfig": {
    "access": "public",
    "directory": "."
  },
  "devDependencies": {
    "@medyll/idae-eslint-config": "workspace:*",
    "@medyll/idae-config-prettier": "latest",
    "@medyll/idae-shared": "workspace:*",
    "@semantic-release/github": "^12.0.5",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "svelte": "^5.50.0",
    "svelte-check": "^4.3.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
